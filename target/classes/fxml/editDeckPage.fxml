            <Image url="@../images/nextBtn.jpg" />
         </image>
         <effect>
            <InnerShadow radius="9.405000000000001" width="18.62">
               <input>
                  <Glow />
               </input>
            </InnerShadow>
         </effect>
      </ImageView>
      <Label fx:id="cardAmountLbl" alignment="CENTER" contentDisplay="CENTER" layoutX="51.0" layoutY="408.0" prefHeight="34.0" prefWidth="195.0" textFill="RED">
