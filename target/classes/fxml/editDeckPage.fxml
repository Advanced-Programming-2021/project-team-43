      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="311.0" layoutY="3.0" prefHeight="34.0" prefWidth="127.0" text="Main Deck" textFill="RED">
         <font>
            <Font name="Bodoni MT" size="18.0" />
         </font></Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="321.0" layoutY="454.0" prefHeight="34.0" prefWidth="107.0" text="Side Deck" textFill="RED">
