            <Font name="Bodoni MT" size="15.0" />
         </font></Label>
      <AnchorPane fx:id="mainDeckPane" layoutX="333.0" layoutY="40.0" prefHeight="410.0" prefWidth="657.0" />
      <AnchorPane fx:id="sideDeckPane" layoutX="338.0" layoutY="494.0" prefHeight="258.0" prefWidth="657.0" />
      <ImageView fx:id="previousCardBtn" fitHeight="26.0" fitWidth="26.0" layoutX="1.0" layoutY="221.0" onMouseClicked="#previousCard" pickOnBounds="true" preserveRatio="true" rotate="180.0">
         <image>
            <Image url="@../images/nextBtn.jpg" />
         </image>
         <effect>
            <InnerShadow radius="9.405000000000001" width="18.62">
               <input>
                  <Glow />
               </input>
            </InnerShadow>
         </effect>
      </ImageView>
   </children>
</AnchorPane>
