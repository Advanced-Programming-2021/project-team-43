<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="lobbyPane" maxHeight="760.0" maxWidth="1000.0" prefHeight="760.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LobbyView">
    <children>
        <ScrollPane fx:id="scrollPane" layoutX="14.0" layoutY="14.0" prefHeight="719.0" prefWidth="471.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="710.0" prefWidth="470.0">
                    <children>
                        <Button fx:id="sendBtn" focusTraversable="false" layoutX="379.0" layoutY="660.0" mnemonicParsing="false" onAction="#sendMessage" prefHeight="28.0" prefWidth="63.0" text="Send">
                            <font>
                                <Font name="Bodoni MT" size="15.0" />
                            </font>
                        </Button>
                        <TextField fx:id="messageTxt" focusTraversable="false" layoutX="6.0" layoutY="660.0" prefHeight="27.0" prefWidth="374.0" promptText="message">
                            <cursor>
                                <Cursor fx:constant="TEXT" />
                            </cursor>
                        </TextField>
                        <VBox fx:id="messageVBox" layoutX="1.0" layoutY="95.0" prefHeight="552.0" prefWidth="458.0" spacing="5.0" />
                        <Label layoutY="-1.0" prefHeight="61.0" prefWidth="139.0" style="-fx-background-color: #006666;" text="Online Users: " textFill="WHITE">
                            <font>
                                <Font name="Bodoni MT" size="22.0" />
                            </font>
                        </Label>
                        <Label fx:id="pinHeadLbl" layoutY="60.0" prefHeight="33.0" prefWidth="456.0" style="-fx-background-color: #00b3b3;" wrapText="true">
                            <font>
                                <Font name="Bodoni MT" size="18.0" />
                            </font>
                        </Label>
                        <Label fx:id="headLbl" layoutX="139.0" layoutY="-1.0" prefHeight="61.0" prefWidth="318.0" style="-fx-background-color: #006666;" textFill="WHITE">
                            <font>
                                <Font name="Bodoni MT" size="22.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <AnchorPane layoutX="589.0" layoutY="14.0" prefHeight="278.0" prefWidth="235.0">
            <children>
                <Label fx:id="findDuelistLbl" alignment="CENTER" contentDisplay="CENTER" layoutX="48.0" layoutY="88.0" onMouseClicked="#findADuelist" prefHeight="39.0" prefWidth="139.0" text="Find Me A Duelist" textAlignment="CENTER">
                    <font>
                        <Font name="Bodoni MT" size="17.0" />
                    </font>
                </Label>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="59.0" layoutY="14.0" prefHeight="29.0" prefWidth="116.0" text="New Game" textAlignment="CENTER">
                    <font>
                        <Font name="Bodoni MT" size="20.0" />
                    </font>
                </Label>
                <RadioButton fx:id="oneRoundBtn" focusTraversable="false" layoutX="33.0" layoutY="61.0" mnemonicParsing="false" text="1 Round">
                    <font>
                        <Font name="Bodoni MT" size="14.0" />
                    </font>
                    <toggleGroup>
                        <ToggleGroup fx:id="x" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="threeRoundBtn" focusTraversable="false" layoutX="136.0" layoutY="61.0" mnemonicParsing="false" text="3 Round" toggleGroup="$x">
                    <font>
                        <Font name="Bodoni MT" size="14.0" />
                    </font>
                </RadioButton>
                <Label fx:id="duelWithAILbl" alignment="CENTER" contentDisplay="CENTER" layoutX="66.0" layoutY="140.0" onMouseClicked="#playWithAI" prefHeight="30.0" prefWidth="104.0" text="Duel with AI" textAlignment="CENTER">
                    <font>
                        <Font name="Bodoni MT" size="17.0" />
                    </font>
                </Label>
                <Label fx:id="messageLbl" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="182.0" prefHeight="39.0" prefWidth="235.0" textAlignment="CENTER">
                    <font>
                        <Font name="Bodoni MT" size="15.0" />
                    </font>
                </Label>
                <Button fx:id="cancelBtn" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" layoutX="85.0" layoutY="230.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" visible="false">
                    <font>
                        <Font name="Bodoni MT" size="16.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="userPane" layoutX="624.0" layoutY="426.0" prefHeight="172.0" prefWidth="235.0">
            <children>
                <ImageView fx:id="userImage" fitHeight="101.0" fitWidth="94.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
                <Label fx:id="infoLbl" alignment="CENTER" contentDisplay="CENTER" layoutX="108.0" layoutY="12.0" prefHeight="88.0" prefWidth="116.0" textAlignment="CENTER" wrapText="true">
                    <font>
                        <Font name="Bodoni MT" size="15.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Button focusTraversable="false" layoutX="902.0" layoutY="656.0" mnemonicParsing="false" onAction="#back" prefHeight="30.0" prefWidth="67.0" text="Back">
            <font>
                <Font name="Bodoni MT" size="15.0" />
            </font>
        </Button>
      <Button layoutX="511.0" layoutY="612.0" mnemonicParsing="false" onAction="#updateChats" text="Update" />
      <Button layoutX="692.0" layoutY="380.0" mnemonicParsing="false" onAction="#match" prefHeight="30.0" prefWidth="67.0" text="match" />
      <TextField fx:id="rivalUsernameTxt" layoutX="632.0" layoutY="331.0" prefHeight="30.0" prefWidth="185.0" promptText="rival username">
         <font>
            <Font name="Bodoni MT" size="15.0" />
         </font>
      </TextField>
      <Label fx:id="matchLbl" layoutX="566.0" layoutY="387.0" prefHeight="30.0" prefWidth="77.0" text="" />
    </children>
</AnchorPane>
